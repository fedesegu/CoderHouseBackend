<h2>Crear Tu Usuario</h2>
<form action="/api/users" method="post">
  <input
    type="text"
    name="first_name"
    id="first_name"
    placeholder="Ingresa tu nombre"
    required
  />
  <input
    type="text"
    name="last_name"
    id="last_name"
    placeholder="Ingresa tu apellido"
    required
  />
  <input
    type="mail"
    name="email"
    id="email"
    placeholder="Ingresa tu mail"
    required
  />
  <input
    type="password"
    name="password"
    id="password"
    placeholder="Ingresa tu contrasena"
  />
  <input
    type="number"
    name="age"
    id="age"
    placeholder="Ingresa tu edad"
  />

  <input type="submit" value="CREAR CUENTA" />
</form>